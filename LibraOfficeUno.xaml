<Window x:Class="Event_Horizon.LibraOfficeUno"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Event_Horizon"
        mc:Ignorable="d"
        Title="LibraOfficeUno" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Content="Open a LibraOffice Document" Click="Open_Click"/>
            <Button Content="Replace Text" Click="Replace_Click"/>
        </StackPanel>
    </Grid>
</Window>
